@page
@model ProyectoAE.Pages.GestionDeEventos.ResEvento.DetailsModel

@{
    ViewData["Title"] = "Details";
}

<h2>Detalles</h2>

<div>
    <h4>Evento: @Html.DisplayFor(model => model.Res_Eventos.NombreEvento)</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Res_Eventos.Id)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Res_Eventos.Id)
        </dd>

        <dt>
            Nombre del Evento
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Res_Eventos.NombreEvento)
        </dd>
        <dt>
            Ponente
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Res_Eventos.Orador)
        </dd>
        <dt>
           Fecha de Inicio
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Res_Eventos.FechaInicio)
        </dd>
        <dt>
            Fecha de Finalización
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Res_Eventos.FechaFin)
        </dd>
        <dt>
            Explicación
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Res_Eventos.Explicaciones)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Res_Eventos.Observaciones)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Res_Eventos.Observaciones)
        </dd>

        <dt>
            Página Web
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Res_Eventos.Url)
        </dd>

    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.Res_Eventos.Id" class="btn btn-primary">Editar</a> |
    <a asp-page="./Index" class="btn btn-default">Regresar</a>
</div>
