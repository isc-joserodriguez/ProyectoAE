@page
@model ProyectoBase.Pages.FicGestionEventos.CatProdServEspecifico.DeleteModel

@{
    ViewData["Title"] = "Delete";
}

<h2>Eliminar</h2>

<h3>¿Estas seguro de que deseas eliminar?</h3>
<div>
    <h4>Catálogo de Productos/Servicios Especificos</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Producto o Servicio:
        </dt>
        <dd>
            @Model.getProdServ(Model.cat_prod_serv_especifico.IdProdServ.ToString())
        </dd>
        <dt>
            Clave
        </dt>
        <dd>
            @Html.DisplayFor(model => model.cat_prod_serv_especifico.ClaveProdServEsp)
        </dd>
        <dt>
            Descripción
        </dt>
        <dd>
            @Html.DisplayFor(model => model.cat_prod_serv_especifico.DesProdServEsp)
        </dd>
    </dl>

    <form method="post">
        <input type="hidden" asp-for="cat_prod_serv_especifico.IdProdServEsp" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-page="./Index" asp-route-id="@Model.cat_prod_serv_especifico.IdProdServ" class="btn btn-warning">Regresar a lista</a>
    </form>
</div>
