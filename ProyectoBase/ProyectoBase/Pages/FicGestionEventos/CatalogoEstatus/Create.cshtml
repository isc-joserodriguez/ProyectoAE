@page
@model ProyectoBase.Pages.FicGestionEventos.CatalogoEstatus.CreateModel

@{
    ViewData["Title"] = "Agregar Nuevo";
}

<h2>Agregar</h2>

<h4>Catalogo de Estatus</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="cat_estatus.IdTipoEstatus" class="control-label"></label>
                <input asp-for="cat_estatus.IdTipoEstatus" class="form-control" />
                <span asp-validation-for="cat_estatus.IdTipoEstatus" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="cat_estatus.Clave" class="control-label"></label>
                <input asp-for="cat_estatus.Clave" class="form-control" />
                <span asp-validation-for="cat_estatus.Clave" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="cat_estatus.DesEstatus" class="control-label"></label>
                <input asp-for="cat_estatus.DesEstatus" class="form-control" />
                <span asp-validation-for="cat_estatus.DesEstatus" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="cat_estatus.Activo" class="control-label"></label>
                <input asp-for="cat_estatus.Activo" class="form-control" />
                <span asp-validation-for="cat_estatus.Activo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="Index">Regresar</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
