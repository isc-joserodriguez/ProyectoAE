@page
@model ProyectoBase.Pages.FicGestionEventos.CatalogoEstatus.DetailsModel

@{
    ViewData["Title"] = "Detalles Estatus";
}

<h2>Detalles Estatus</h2>

<div>
    <h4>Catálago Estatus</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            <label class="control-label">Tipo Estatus:</label>
        </dt>
        <dd>
            @Model.TipoEstatus(Model.cat_estatus.IdTipoEstatus.ToString())
        </dd>
        <dt>
            <label class="control-label">Clave:</label>
        </dt>
        <dd>
            @Html.DisplayFor(model => model.cat_estatus.Clave)
        </dd>
        <dt>
            <label class="control-label">Descripción:</label>
        </dt>
        <dd>
            @Html.DisplayFor(model => model.cat_estatus.DesEstatus)
        </dd>
        <dt>
            <label class="control-label">Activo :</label>
        </dt>
        <dd>
            @Model.Activo(Model.cat_estatus.Activo)
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.cat_estatus.IdEstatus" class="btn btn-info">Editar</a>
    <a asp-page="./Index" class="btn btn-warning">Regresar a lista</a>
</div>
