@page
@model ProyectoBase.Pages.FicGestionEventos.Edificios.CreateModel

@{
    ViewData["Title"] = "Create";
}

<h2>Agregar</h2>
<h4>Catálago de Edificios</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">

                <label class="control-label">Clave:</label>
                <input asp-for="eva_cat_edificios.Clave" class="form-control" />
                <span asp-validation-for="eva_cat_edificios.Clave" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label class="control-label">Alias:</label>
                <input asp-for="eva_cat_edificios.Alias" class="form-control" />
                <span asp-validation-for="eva_cat_edificios.Alias" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label class="control-label">Descripción:</label>
                <textarea asp-for="eva_cat_edificios.DesEdificio" class="form-control" ></textarea>
                <span asp-validation-for="eva_cat_edificios.DesEdificio" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label class="control-label">Prioridad:</label>
                <input asp-for="eva_cat_edificios.Prioridad" type="number" class="form-control" />
                <span asp-validation-for="eva_cat_edificios.Prioridad" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Guardar" class="btn btn-info" /> |
                <a asp-page="Index" class="btn btn-warning">Regresar a lista</a>
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
