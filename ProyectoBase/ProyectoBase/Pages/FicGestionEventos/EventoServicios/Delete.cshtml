@page
@model ProyectoBase.Pages.FicGestionEventos.EventoServicios.DeleteModel

@{
    ViewData["Title"] = "Delete";
}

<h2>Eliminar</h2>

<h3>¿Seguro quieres Eliminar Esto?</h3>
<div>
    <h4>Servicios de Eventos</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Evento:
        </dt>
        <dd>
            @Model.Evento(Model.res_evento_servicios.IdEvento.ToString())
        </dd>
        <dt>
            Requerido:
        </dt>
        <dd>
            @Model.getRequerido(Model.res_evento_servicios.Requerido)
        </dd>
        <dt>
            Producto/Servicio:
        </dt>
        <dd>
            @Model.getProductoServicoEsp(Model.res_evento_servicios.IdProdServEsp.ToString(),Model.res_evento_servicios.IdProdServ.ToString())
        </dd>
    </dl>
    
    <form method="post">
        <input type="hidden" asp-for="res_evento_servicios.IdEvento" />
        <input type="hidden" asp-for="res_evento_servicios.Requerido" />
        <input type="hidden" asp-for="res_evento_servicios.IdProdServ" />
        <input type="hidden" asp-for="res_evento_servicios.IdProdServEsp" />
        <input type="submit" value="Eiminar" class="btn btn-danger" />
        <a asp-page="./Index" asp-route-id="@Model.IdEvento" class="btn btn-warning">Regresar a la Lista</a>
    </form>
</div>
