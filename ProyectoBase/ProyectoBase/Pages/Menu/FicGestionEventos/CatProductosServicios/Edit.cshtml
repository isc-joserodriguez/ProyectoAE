@page
@model ProyectoBase.Pages.Menu.FicGestionEventos.CatProductoServicios.EditModel

@{
    ViewData["Title"] = "Edit";
}

<h2>Editar Catálogo [@Html.DisplayFor(model => model.cat_productos_servicios.IdProdServ)]</h2>

<h4>Catálogo de Productos y Servicios</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="cat_productos_servicios.IdProdServ" />
            <div class="form-group">
                <label asp-for="cat_productos_servicios.ClaveProdServ" class="control-label"></label>
                <input asp-for="cat_productos_servicios.ClaveProdServ" class="form-control" />
                <span asp-validation-for="cat_productos_servicios.ClaveProdServ" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="cat_productos_servicios.CodigoBarras" class="control-label"></label>
                <input asp-for="cat_productos_servicios.CodigoBarras" class="form-control" />
                <span asp-validation-for="cat_productos_servicios.CodigoBarras" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="cat_productos_servicios.DesProdServ" class="control-label"></label>
                <input asp-for="cat_productos_servicios.DesProdServ" class="form-control" />
                <span asp-validation-for="cat_productos_servicios.DesProdServ" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="cat_productos_servicios.ProductoServicio" class="control-label"></label>
                <input asp-for="cat_productos_servicios.ProductoServicio" class="form-control" />
                <span asp-validation-for="cat_productos_servicios.ProductoServicio" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="cat_productos_servicios.IdTipoGenProdServ" class="control-label"></label>
                <input asp-for="cat_productos_servicios.IdTipoGenProdServ" class="form-control" />
                <span asp-validation-for="cat_productos_servicios.IdTipoGenProdServ" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="cat_productos_servicios.IdGenProdServ" class="control-label"></label>
                <input asp-for="cat_productos_servicios.IdGenProdServ" class="form-control" />
                <span asp-validation-for="cat_productos_servicios.IdGenProdServ" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Guardar" class="btn btn-primary" /> |
                <a asp-page="./Index" class="btn btn-default">Regresar a lista</a>
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
