@page
@model ProyectoBase.Pages.Menu.FicGestionEventos.CatTiposEstatus.CreateModel

@{
    ViewData["Title"] = "Create";
}

<h2>Agregar Catálogo</h2>

<h4>Catálogo Tipo Estatus</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="cat_tipos_estatus.DesTipoEstatus" class="control-label">Descripción</label>
                <input asp-for="cat_tipos_estatus.DesTipoEstatus" class="form-control" />
                <span asp-validation-for="cat_tipos_estatus.DesTipoEstatus" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="cat_tipos_estatus.Activo" class="control-label"></label>
                <input asp-for="cat_tipos_estatus.Activo" class="form-control" />
                <span asp-validation-for="cat_tipos_estatus.Activo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Crear" class="btn btn-primary" /> |
                <a asp-page="Index" class="btn btn-default">Regresar a lista</a>
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
