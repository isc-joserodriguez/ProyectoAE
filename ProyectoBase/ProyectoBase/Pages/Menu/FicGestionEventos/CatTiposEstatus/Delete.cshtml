@page
@model ProyectoBase.Pages.Menu.FicGestionEventos.CatTiposEstatus.DeleteModel

@{
    ViewData["Title"] = "Delete";
}

<h2>Eliminar Catálogo</h2>

<h3>¿Estas seguro de que deseas eliminar este catálogo?</h3>
<div>
    <h4>Catálago Tipo Estatus</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            <label class="control-label">Descripción:</label>
        </dt>
        <dd>
            @Html.DisplayFor(model => model.cat_tipos_estatus.DesTipoEstatus)
        </dd>
        <dt>
            <label class="control-label">Activo:</label>
        </dt>
        <dd>
            @Model.Activo(Model.cat_tipos_estatus.Activo)
        </dd>
    </dl>

    <form method="post">
        <input type="hidden" asp-for="cat_tipos_estatus.IdTipoEstatus" />
        <input type="submit" value="Eliminar" class="btn btn-info" />
        <a asp-page="./Index" class="btn btn-warning">Regresar a lista</a>
    </form>
</div>
