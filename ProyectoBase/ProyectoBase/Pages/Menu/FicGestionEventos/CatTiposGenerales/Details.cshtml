@page
@model ProyectoBase.Pages.Menu.FicGestionEventos.CatTiposGenerales.DetailsModel

@{
    ViewData["Title"] = "Details";
}

<h2>Detalles</h2>

<div>
    <h4>Catálago Tipos de Evento</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            <label class="control-label">Descripción:</label>
        </dt>
        <dd>
            @Html.DisplayFor(model => model.cat_tipos_generales.DesTipo)
        </dd>
        <dt>
            <label class="control-label">Activo:</label>
        </dt>
        <dd>
            @Model.Activo(Model.cat_tipos_generales.Activo)
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.cat_tipos_generales.IdTipoGeneral" class="btn btn-info">Editar</a>
    <a asp-page="./Index" class="btn btn-warning">Regresar a lista</a>
</div>
