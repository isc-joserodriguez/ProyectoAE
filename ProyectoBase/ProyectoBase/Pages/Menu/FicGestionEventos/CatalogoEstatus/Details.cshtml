}

<h2>Detalles Estatus</h2>

<div>
    <h4>Catalago Estatus</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
           Id Tipo Estatus
        </dt>
        <dd>
            @Html.DisplayFor(model => model.cat_estatus.IdTipoEstatus)
