@page
@model ProyectoBase.Pages.FicGestionEventos.CatalogoGenerales.CreateModel

@{
    ViewData["Title"] = "Create";
}

<h2>Agregar Catálogo</h2>

<h4>Catálogo Generales</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="cat_generales.IdTipoGeneral" class="control-label">Id Tipo General</label>
                <input asp-for="cat_generales.IdTipoGeneral" class="form-control" />
                <span asp-validation-for="cat_generales.IdTipoGeneral" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="cat_generales.Clave" class="control-label">Clave</label>
                <input asp-for="cat_generales.Clave" class="form-control" />
                <span asp-validation-for="cat_generales.Clave" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="cat_generales.DesGeneral" class="control-label">Descripción</label>
                <input asp-for="cat_generales.DesGeneral" class="form-control" />
                <span asp-validation-for="cat_generales.DesGeneral" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="cat_generales.Activo" class="control-label">Activo</label>
                <input asp-for="cat_generales.Activo" class="form-control" />
                <span asp-validation-for="cat_generales.Activo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Crear" class="btn btn-primary" /> |
                <a asp-page="Index" class="btn btn-default">Regresar a lista</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
