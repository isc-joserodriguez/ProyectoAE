@page
@model ProyectoBase.Pages.Menu.FicGestionEventos.CatalogoGenerales.DetailsModel

@{
    ViewData["Title"] = "Details";
}

<h2>Detalles</h2>

<div>
    <h4>Catálago de Tipos</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            <label class="control-label">Id Tipo General:</label>
        </dt>
        <dd>
            @Html.DisplayFor(model => model.cat_generales.IdTipoGeneral)
        </dd>
        <dt>
            <label class="control-label">Clave:</label>
        </dt>
        <dd>
            @Html.DisplayFor(model => model.cat_generales.Clave)
        </dd>
        <dt>
            <label class="control-label">Descripción:</label>
        </dt>
        <dd>
            @Html.DisplayFor(model => model.cat_generales.DesGeneral)
        </dd>
        <dt>
            <label class="control-label">Activo:</label>
        </dt>
        <dd>
            @Model.Activo(Model.cat_generales.Activo)
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.cat_generales.IdGeneral" class="btn btn-primary">Editar</a> |
    <a asp-page="./Index" class="btn btn-default">Regresar a lista</a>
</div>
