@page
@model ProyectoBase.Pages.Menu.FicGestionEventos.Edificios.DeleteModel

@{
    ViewData["Title"] = "Delete";
}

<h2>Eliminar</h2>

<h3>¿Estas seguro de que deseas eliminar este edificio?</h3>
<div>
    <h4>Catálogo de Edificios</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            <label class="control-label">Clave:</label>
        </dt>
        <dd>
            @Html.DisplayFor(model => model.eva_cat_edificios.Clave)
        </dd>
        <dt>
            <label class="control-label">Alias:</label>
        </dt>
        <dd>
            @Html.DisplayFor(model => model.eva_cat_edificios.Alias)
        </dd>
        <dt>
            <label class="control-label">Descripción:</label>
        </dt>
        <dd>
            @Html.DisplayFor(model => model.eva_cat_edificios.DesEdificio)
        </dd>
        <dt>
            <label class="control-label">Prioridad:</label>
        </dt>
        <dd>
            @Html.DisplayFor(model => model.eva_cat_edificios.Prioridad)
        </dd>
    </dl>

    <form method="post">
        <input type="hidden" asp-for="eva_cat_edificios.IdEdificio" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> |
        <a asp-page="./Index" class="btn btn-warning">Regresar a lista</a>
    </form>
</div>
