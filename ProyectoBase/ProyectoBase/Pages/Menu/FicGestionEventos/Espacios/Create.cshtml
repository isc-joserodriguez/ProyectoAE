@page
@model ProyectoBase.Pages.FicGestionEventos.Espacios.CreateModel

@{
    ViewData["Title"] = "Create";
}

<h2>Agregar Espacio</h2>

<h4>Catálogo Espacios</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="eva_cat_espacios.IdEdificio" class="control-label">Id</label>
                <input asp-for="eva_cat_espacios.IdEdificio" class="form-control" />
                <span asp-validation-for="eva_cat_espacios.IdEdificio" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="eva_cat_espacios.Clave" class="control-label"></label>
                <input asp-for="eva_cat_espacios.Clave" class="form-control" />
                <span asp-validation-for="eva_cat_espacios.Clave" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="eva_cat_espacios.Alias" class="control-label"></label>
                <input asp-for="eva_cat_espacios.Alias" class="form-control" />
                <span asp-validation-for="eva_cat_espacios.Alias" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="eva_cat_espacios.DesEspacio" class="control-label">Descripción</label>
                <input asp-for="eva_cat_espacios.DesEspacio" class="form-control" />
                <span asp-validation-for="eva_cat_espacios.DesEspacio" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="eva_cat_espacios.Capacidad" class="control-label"></label>
                <input asp-for="eva_cat_espacios.Capacidad" class="form-control" />
                <span asp-validation-for="eva_cat_espacios.Capacidad" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="eva_cat_espacios.Prioridad" class="control-label"></label>
                <input asp-for="eva_cat_espacios.Prioridad" class="form-control" />
                <span asp-validation-for="eva_cat_espacios.Prioridad" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Crear" class="btn btn-primary" /> |
                <a asp-page="Index" class="btn btn-default">Regresar a lista</a>
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
